{
	"name": "power-fsm-viewer",
	"displayName": "Power FSM Viewer",
	"description": "A viewer for javascript finite state machine via Graphviz",
	"version": "1.0.5",
	"publisher": "ThankCreate",
	"license": "MIT",
	"engines": {
		"vscode": "^1.36.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thankcreate/power-fsm-viewer.git"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onCommand:powerFsmViewer.open"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "powerFsmViewer.open",
				"title": "FSM View: Open"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "run-p compile:*",
		"watch": "run-p watch:*",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"compile:Src": "tsc -p ./",
		"compile:View": "tsc -p ./tsconfig.view.json",
		"watch:Src": "tsc -watch -p ./",
		"watch:View": "tsc -watch -p ./tsconfig.view.json"
	},
	"devDependencies": {
		"@babel/types": "^7.5.5",
		"@types/babel__core": "^7.1.2",
		"@types/ejs": "^2.6.3",
		"@types/glob": "^7.1.1",
		"@types/jquery": "^3.3.30",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.13",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"npm-run-all": "^4.1.5",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/generator": "^7.4.0",
		"@babel/parser": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-typescript": "^7.5.5",
		"ejs": "^2.6.2"
	}
}
